{
  "name": "t3-startup",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "next dev",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "db:clean": "shx rm -rf node_modules/.prisma && shx rm -rf prisma/db.sqlite* && shx rm -rf prisma/migrations/*_init",
    "db:init": "prisma migrate dev --schema prisma/schema.prisma --name init",
    "db:seed": "ts-node --esm prisma/seed.ts",
    "db:test": "ts-node --esm prisma/test.ts",
    "db:reset": "npm run db:clean && npm run db:init && npm run db:seed"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.6.0",
    "@t3-oss/env-nextjs": "^0.7.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.43.6",
    "@trpc/next": "^10.43.6",
    "@trpc/react-query": "^10.43.6",
    "@trpc/server": "^10.43.6",
    "@types/nodemailer": "^6.4.14",
    "date-fns": "^3.2.0",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "nodemailer": "^6.9.8",
    "playwright": "1.40.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^9.0.1",
    "react-select": "^5.8.0",
    "server-only": "^0.0.1",
    "shx": "^0.3.4",
    "superjson": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@playwright/test": "1.40.0",
    "@types/eslint": "^8.44.7",
    "@types/node": "^18.17.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.1.6"
  },
  "ct3aMetadata": {
    "initVersion": "7.25.1"
  },
  "packageManager": "pnpm@8.14.0"
}
